{
  "name": "lunch",
  "version": "0.0.1",
  "description": "Get some lunch",
  "scripts": {
    "clean": "node tasks/clean",
    "static-assets": "node tasks/static-assets",
    "build": "npm run static-assets && npm run build:server",
    "build:server": "gulp build:server",
    "build:client": "webpack --config webpack.config.prod.js",
    "build:production": "npm run clean && npm run build && npm run build:client",
    "start": "npm run build && node out/server/index.js",
    "start:production": "npm run build:production && node out/server/index.js",
    "test": "npm run test:unit && npm run test:lint",
    "test:unit": "webpack --config webpack.config.test.js && mocha out/test/**/*.spec.js",
    "test:lint": "eslint src/**/*.js test/**/*.js gulpfile.js webpack.config.*"
  },
  "author": "Dean Merchant <deanmerchant@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git@github.com:Dean177/lunch.git"
  },
  "license": "ISC",
